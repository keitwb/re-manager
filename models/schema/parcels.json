{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "rem-parcel-info",
  "title": "ParcelInfo",
  "description": "Information about a specific parcel",
  "type": "object",
  "allOf": [
    {
      "properties": {
        "pinNumber": { "type": "string" },
        "ownerName": { "type": "string" },
        "acreage": { "type": "number" },
        "streetAddress": { "type": "string" },
        "boundaryWKT": { "type": "string" }
      }
    }
  ]
}
